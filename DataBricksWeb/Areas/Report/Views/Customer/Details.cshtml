@model DataBricksWeb.Database.Customer
@{
    ViewBag.Title = "Index";
}

<section class="content-header">
    <h1>
        Customer

    </h1>
    <ol class="breadcrumb">
        <li><a href="/dashboard/landingpage"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="/partners/list">Customers List</a></li>
        <li class="active">Customer</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <!-- SELECT2 EXAMPLE -->
    <div class="box box-default">
        <!-- /.box-header -->
        <div class="box-body">
            <div class="row">

                <div class="col-md-6">
                    <div class="form-group">
                        <label>Customer Name<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.CustomerName</span>
                            <input type="hidden" name="Id" value="@Model.Id" />
                            <input type="hidden" name="IsApproved" id="hdnApproveFlag" />
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label>Branch<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.Branch</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label>Email Id<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.EmailId</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label>Country<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.CountryName</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label>City<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.CityName</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label>Zip<span style="color:red;">*</span></label>
                        <div>
                            <span>@Model.Zip</span>
                        </div>
                    </div>
                </div>
            </div>

        </div>


        <div class="box-footer">
            <div class="col-lg-12 p-t-20 text-center">
                <input type="button" onclick="pageloade();" class="btn btn-danger" style="background-color: #dc3545; border-color: #dc3545;margin-left: 10px;" id="btnReject" value="Cancel" />
            </div>
        </div>
    </div>


</section>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/CustomScript.js"></script>
<script src="~/Scripts/bootstrap.js"></script>
<script>
    function pageloade() {
        window.location.href = "@Url.Action("List","Customer",new { area = "Report"})";
    };
    var v = document.getElementById("Customersnew");
    v.className += "tabcolor";
</script>