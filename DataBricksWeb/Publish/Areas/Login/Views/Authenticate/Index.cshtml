
@{
    ViewBag.Title = "Partner Login";
    Layout = "~/Areas/Login/Views/Shared/_Layout.cshtml";
}


<style>
    .brand-wrapper .logo {
        height: 90px;
    }
    </style>


<main class="d-flex align-items-center min-vh-100 py-3 py-md-0">
    <div class="container">
        <div class="card login-card">
            <div class="row no-gutters">
                <div class="col-md-5">
                    <img src="~/NewLogin/assets/images/login.jpg" alt="login" class="login-card-img">
                </div>
                <div class="col-md-7">
                    <div class="card-body">
                        <div class="brand-wrapper">
                            <img src="/NewAdmin/DB_LOGO.png" alt="logo" class="logo">
                        </div>
                        <p class="login-card-description">Sign into your account</p>
                        <form method="post" action="@Url.Action("Index","Authenticate",new { area = "Login"})" enctype="multipart/form-data">
                            <div class="form-group">
                                <label for="email" class="sr-only">Email</label>
                                <input type="email" id="txtEmailId" name="email" maxlength="256" class="form-control" placeholder="Email address">
                            </div>
                            <div class="form-group mb-4">
                                <label for="password" class="sr-only">Password</label>
                                <input type="password" id="txtPassword" name="password" class="form-control" placeholder="***********">
                            </div>
                            <input name="login" id="login" class="btn btn-block login-btn mb-4" type="submit" value="Login" style="color:white;background-color:#000;">
                        </form>
                        <b><u><a href="@Url.Action("registration", "partner",new { area = "Login"})"> Register Here</a></u></b>
                        <b><u><a href="@Url.Action("ForgotPassword", "Authenticate",new { area = "Login"})" style="margin-left: 22%;">Forgot Password?</a></u></b>
                            
                            @*<p class="login-card-footer-text">Don't have an account? <a href="#!" class="text-reset">Register here</a></p>*@

                    </div>
                </div>
            </div>
        </div>
