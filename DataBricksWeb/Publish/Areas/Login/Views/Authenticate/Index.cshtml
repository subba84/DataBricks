<script>
     if ("@TempData["Notification"]" != null && "@Convert.ToString(TempData["Notification"])" != "") {
        alert("@Convert.ToString(TempData["Notification"])");
    }
    $("#btnLogin").click(function () {
        debugger;
        var email = $("#txtEmailId").val().trim();
        var password = $("#txtPassword").val().trim();
        if (fnIsNullorEmpty(email)) {
            Warning("Please enter register email id");
            return false;
        }
        if (fnIsNullorEmpty(password)) {
            Warning("Please enter password");
            return false;
        }
    });

    $("#btnRegister").click(function () {
        window.location.href = "@Url.Action("Index","Registration",new { area = "Partner"})";
    });
</script>